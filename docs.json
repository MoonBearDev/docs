{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "DARO DOCS",
  "colors": {
    "primary": "#6300da",
    "light": "#9747ff",
    "dark": "#5400b9"
  },
  "favicon": "/daro-favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "다로 시작하기",
            "pages": [
              "before-the-beginning",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "SDK 통합하기",
            "pages": [
              {
                "group": "Android",
                "icon": "android",
                "pages": [
                  "essentials/markdown",
                  "essentials/code",
                  "essentials/images"
                ],
                "group": "iOS",
                "icon": "apple",
                "pages": [
                  "essentials/settings",
                  "essentials/navigation",
                  "essentials/reusable-snippets"
                ]
              }
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint Examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://docs.daro.so",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Blog",
          "href": "https://daro.so/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@daro.so"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.daro.so"
    }
  },
  "footer": {
    "socials": {
      "linkedin": "https://linkedin.com/company/delightroom"
    }
  }
}